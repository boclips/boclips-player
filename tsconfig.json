{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es2015", "es6", "dom"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "plugins": [{ "name": "typescript-tslint-plugin" }],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
